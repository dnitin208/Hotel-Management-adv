    <div class="Divv">

    <!-- <br> -->
    <br>
    <br>
    <h1 class="heading">
        Allotment of Room
    </h1>
    <br>
    <br>

    <!-- ................. -->

    
    <form class="Form-add" method="post" action="/add">
        <div class="form-group row">
          <label for="inputEmail3" class="col-sm-2 col-form-label">Name</label>
          <div class="col-sm-4">
            <input type="text" class="form-control" id="inputEmail3" placeholder="Name" name="nam">
          </div>
        </div>


        <div class="form-group row">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Email</label>
            <div class="col-sm-4">
              <input type="email" class="form-control" id="inputEmail3" placeholder="abc@example.com" name="emai">

            </div>
          </div>

          <div class="form-group row">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Contact</label>
            <div class="col-sm-4">
              <input type="text" class="form-control" id="inputEmail3" placeholder="Mobile Number" name="contac">
            </div>
          </div>
          
          <div class="form-group row">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Aadhar No.</label>
            <div class="col-sm-4">
              <input type="text" class="form-control" id="inputEmail3" placeholder="Aadhar Number" name="aadha">
            </div>
          </div>

          <div class="form-group row">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Count</label>
            <div class="col-sm-4">
              <input type="text" class="form-control" id="inputEmail3" placeholder="1/2/3/4" name="coun">
            </div>
          </div>

        

        <div class="form-group row">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Accomodation</label>
            <div class="col-sm-4">
              <input type="text" class="form-control" id="inputEmail3" placeholder="AC/NAC" name="accomodatio">
            </div>
          </div>

          <div class="form-group row">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Date</label>
            <div class="col-sm-4">
              <input type="text" class="form-control" id="inputEmail3" placeholder="mm-dd-yyyy" name="dat">
            </div>
          </div>

          <div class="form-group row">
            <label for="inputEmail3" class="col-sm-2 col-form-label">No. of days</label>
            <div class="col-sm-4">
              <input type="text" class="form-control" id="inputEmail3" placeholder="1/2/3/4...." name="day">
            </div>
          </div>

        <!-- <br> -->
        <div class="form-group row">
          <div class="col-sm-10 btn1">
            <button type="submit" class="btn btn-primary">ALLOT</button>
          </div>
        </div>


        <div class="form-group row">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Room No.</label>
            <div class="col-sm-4">
              <input type="text" class="form-control" placeholder="<%=room_no%>" value="<%=room_no%>" id="inputEmail3" name="roo">
            </div>
          </div>
      
          <div class="form-group row">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Amount</label>
            <div class="col-sm-4">
              <input type="text" class="form-control" id="inputEmail3" placeholder="<%=amount%>">
            </div>
          </div>


      </form>


      <script src="add.js"></script>

    </div>

</body>
